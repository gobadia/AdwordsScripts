/**************************************************************************************
* AdWords - Optimization -- Ad Serving Control Check 
* This script tests search queries to determine if multiple ad groups are showing, which  
* indicates that ad serving control has been lost. 
* Version 1.1
* Created By: Derek Martin
* DerekMartinLA.com
* Modified By:  Gabriel Obadia
**************************************************************************************/

var spreadsheetURL = "https://docs.google.com/spreadsheets/d/1Sd8ccZNvbQgW7HlpZHyPta7acxtYY7Bs_g6f6DGhSRI/"; // Replace URL between Quotes with Google Sheet URL
var spreadsheetTab = "Report"; // Replace tab name between Quotes with the name of the tab in the Google Sheet above

// Calculates Last 7 Day Dates
var d = new Date();
var eightDaysAgo = eightDayDate(d);
var yesterday = yesterdayDate(d);
// End Date Calculations

var FROM_DATE = eightDaysAgo.toString(); // To select manual dates replace eightDaysAgo.toString() with 'YYYYMMDD'
var TO_DATE = yesterday.toString(); // To select manual dates replace yesterday.toString() with 'YYYYMMDD'


function main() {

  Logger.log("From Date: " + eightDaysAgo);
  Logger.log("To Date: " + yesterday);

  
  var allQueries = [];
  var listOfAdGroups = [];
  var ss = SpreadsheetApp.openByUrl(spreadsheetURL);
  var sheet = ss.getSheetByName(spreadsheetTab);
  sheet.clear();
  Logger.log('Analyzing Ad Group conflicts now...');
  
  allQueries = getQueries();
    
  var i = 0;
  var h = 1;
  for each(query in allQueries) {
    var testQueryList = _.where(allQueries, {query: allQueries[i].query})
    if (testQueryList.length > 1) {
      for (k = 0; k < testQueryList.length; k++) {
         listOfAdGroups.push(testQueryList[k].adGroup);
        }
      } // end of for statement
       listOfAdGroups = _.uniq(listOfAdGroups);
       
      if (listOfAdGroups.length > 1) {                            
              // Build Array of Conflicts
              var conflictData = [];
              conflictData.push(
                allQueries[i].query.toString(),
                allQueries[i].keyword.toString()
              );
              //Loop through Conflicting Ad Groups
              for (var z = 0; z < listOfAdGroups.toString().split(',').length; z++) {
              conflictData.push(listOfAdGroups[z]);              
              };
              if ( h === 1) {
                var headers = ["Query", "Keyword", "Ad Groups"];
                var headerRange = sheet.getRange(1,1,1,headers.length);
              headerRange.setValues([headers]);
                Logger.log("Column Headers Inserted");
              };
              var spreadsheetRange = sheet.getRange(1+h,1,1,conflictData.length);
              spreadsheetRange.setValues([conflictData]);
        h++;
    } // end of test if 
    i++;
    listOfAdGroups = [];
  } // end of for each
  
  Logger.log("Analyzing Ad Group Conflicts Complete. Please See Google Sheet.");
} // end of main function

function yesterdayDate (date) {
   var yyyy = date.getFullYear().toString();
   var mm = (date.getMonth()+1).toString(); // getMonth() is zero-based
   var dd  = (date.getDate()-1).toString();
   return yyyy + (mm.length===2?mm:"0"+mm[0]) + (dd[1]?dd:"0"+dd[0]); // padding
}

function eightDayDate (date) {
   var yyyy = date.getFullYear().toString();
   var mm = (date.getMonth()+1).toString(); // getMonth() is zero-based
   var dd  = (date.getDate()-8).toString();
   return yyyy + (mm.length===2?mm:"0"+mm[0]) + (dd[1]?dd:"0"+dd[0]); // padding
}
function getQueries(camp) {
    var camp = camp;
    var reportFormat = _.str.quote(camp);
    var listOfQueries = [];
  
  var report = AdWordsApp.report(
     'SELECT Query, KeywordTextMatchingQuery, AdGroupName, Cost ' +
     'FROM   SEARCH_QUERY_PERFORMANCE_REPORT ' +
     'WHERE AdGroupStatus = ENABLED ' +

     'DURING ' +FROM_DATE + ','+ TO_DATE
      );
    
     var rows = report.rows();

   while (rows.hasNext()) {
       var row = rows.next();
       
       var query = row['Query'];
       var keyword = row['KeywordTextMatchingQuery'];
       var adGroup = row['AdGroupName'];
       
           var searchQuery = new searchTerm(query, keyword, adGroup);
           
           listOfQueries.push(searchQuery);
            
              
   }  // end of report run
   return _.uniq(listOfQueries);
    
} 

function searchTerm (query, keyword, adGroup) {
  this.query = query;
  this.keyword = keyword;
  this.adGroup = adGroup;
  
} // end of searchTerm

/* UNDERSCORE LIBRARIES */

